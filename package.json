{
  "name": "XProjectReact",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.4.1",
    "react-dom": "^16.10.2",
    "react-native": "0.56.0",
    "react-navigation": "^2.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/runtime": "^7.9.6",
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "5.0.2",
    "detox": "14.9.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "express": "^4.17.1",
    "jest": "23.5.0",
    "mocha": "^6.2.2",
    "react-native-testing-library": "^1.13.2",
    "react-test-renderer": "16.4.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/setup-tests.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "detox": {
    "test-runner": "mocha",
    "runner-config": "e2e/mocha.opts",
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/app-debug.apk",
        "testBinaryPath": "android/app/build/outputs/apk/app-debug-androidTest.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Pixel_XL_API_28"
      },
      "android.att.debug": {
        "binaryPath": "android/app/build/outputs/apk/app-debug.apk",
        "testBinaryPath": "android/app/build/outputs/apk/app-debug-androidTest.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.attached",
        "name": "68U5T17A17004871"
      }
    }
  }
}
